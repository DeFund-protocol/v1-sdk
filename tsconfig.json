{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "declaration": true,
        "outDir": "./lib/",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "lib": [
            "ESNext"
        ],
    },
    "include": [
        "src",
    ],
    "exclude": [
        "node_modules",
        "lib",
    ]
}