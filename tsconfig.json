{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "declaration": true,
        "outDir": "./lib/",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
    },
    "include": [
        "src",
    ],
    "exclude": [
        "node_modules",
        "lib",
    ]
}